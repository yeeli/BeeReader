    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.1",
    "devtron": "^1.4.0",
    "electron": "^1.6.6",
    "electron-builder": "^17.3.1",
    "electron-debug": "^1.2.0",
