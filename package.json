{
  "name": "BeeReader",
  "productName": "BeeReader",
  "version": "1.0.0",
  "main": "src/main/index.js",
  "author": "yeeli",
  "license": "MIT",
  "scripts": {
    "hs": "cross-env NODE_ENV=development webpack-dev-server --config webpack/development.js",
    "dev": "cross-env HOT=1 NODE_ENV=development electron . --dev",
    "dev:server": "cross-env HOT=1 NODE_ENV=development nodemon ./src/main/server.js",
    "build:pro": "cross-env NODE_ENV=production node_modules/.bin/babel-node --trace-warnings ./node_modules/.bin/webpack --config webpack/production.js",
    "pro": "cross-env NODE_ENV=production electron  .",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "migration": "knex --knexfile src/config/database.js --cwd src/db/ migrate:make",
    "rollback": "knex --knexfile src/config/database.js --cwd src/db/ migrate:rollback",
    "db:dev:migrate": "cross-env NODE_ENV=development NODE_FROM=console node ./src/db/migrate.js",
    "db:dev:seeds": "cross-env NODE_ENV=development NODE_FROM=console node ./src/db/seeds.js",
    "package": "electron-packager . BeeReader --platform=darwin",
    "package:mac": "cross-env NODE_ENV=production  electron-packager . BeeReader --platform=darwin --overwrite"
  },
  "devDependencies": {
