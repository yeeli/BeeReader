{
  "name": "BeeReader",
  "version": "1.0.0",
  "main": "main/index.js",
  "author": "yeeli",
  "license": "MIT",
  "scripts": {
    "hs": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack/development.js",
    "dev": "cross-env HOT=1 NODE_ENV=development electron  -r babel-register -r babel-polyfill ./src/main/index.js --dev --enable-logging",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "migration": "knex --knexfile src/config/database.js --cwd src/db/ migrate:make",
    "db:dev:migrate": "cross-env NODE_ENV=development node -r babel-register ./src/db/schema.js"
  },
